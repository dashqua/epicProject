FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (.8944 .4472 0);
//#include "init/internalU"

boundaryField
{
    inlet
    {
      //#include "init/boundaryUinlet"
      type            slip;//noSlip;
      /*
      type fixedValue;
      value uniform (.8944 4472 0);
      */
    }
    outlet
    {
      type            slip;//noSlip;
    }
    left
    {
      type            symmetryPlane;//noSlip;
    }
    right
    {
      type            symmetryPlane;//noSlip;
    }
    top
    {
      type            symmetryPlane;//noSlip;
    }
    bottom
    {
      type            symmetryPlane;//noSlip;
    }      
    defaultFaces
    {
      type            empty;//noSlip;
    }
}


