FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 3;

boundaryField
{
    inlet
    {
        type            cyclic;
        value           uniform 3;
    }
    outlet
    {
        type            cyclic;
        value           uniform 3;
    }
    top
    {
        type            cyclic;
        value           uniform 3;
    }
    bottom
    {
        type            cyclic;
        value           uniform 3;
    }
    left
    {
        type            cyclic;
        value           uniform 3;
    }
    right
    {
        type            cyclic;
        value           uniform 3;
    }
}


