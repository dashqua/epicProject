FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 293.15;

boundaryField
{
    sides
    {
        type            calculated;
        value           uniform 293.15;
    }
    top
    {
        type            calculated;
        value           uniform 293.15;
    }
    inlet
    {
        type            calculated;
        value           uniform 293.15;
    }
    bottom
    {
        type            calculated;
        value           uniform 293.15;
    }
    outlet
    {
        type            calculated;
        value           uniform 293.15;
    }
    ring
    {
        type            calculated;
        value           uniform 293.15;
    }
    defaultFaces
    {
        type            calculated;
        value           uniform 293.15;
    }
}


