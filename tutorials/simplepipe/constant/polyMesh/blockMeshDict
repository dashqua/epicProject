FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant/polyMesh";
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 0.001;

vertices
(
	(-10	-7.5	0)	//vertex 0
	(10	-7.5	0)	//vertex 1
	(10	7.5	0)	//vertex 2
	(-10	7.5	0)	//vertex 3
	(-10	-7.5	1)	//vertex 4
	(10	-7.5	1)	//vertex 5
	(10	7.5	1)	//vertex 6
	(-10	7.5	1)	//vertex 7
);

edges
(
	// if nothing is specified,
	// bMD uses lines
);

blocks
(
	hex (0 1 2 3 4 5 6 7)
	(10 10 1)
	simpleGrading (1 1 1)
);

boundary
(
	inbound
	{
		type		cyclicAMI;
		neighbourPatch	outbound;
		faces		((0 3 7 4));
	}
	outbound
	{
		type		cyclicAMI;
		neighbourPatch	inbound;
		faces		((1 2 6 5));
	}
	left
	{
		type		cyclicAMI;
		neighbourPatch	right;
		faces		((0 1 2 3));
	}
	right
	{
		type		cyclicAMI;
		neighbourPatch	left;
		faces 		((4 5 6 7));
	}
	top
	{
		type		cyclicAMI;
		neighbourPatch	bottom;
		faces		((2 3 7 6));
	}
	bottom
	{
		type		cyclicAMI;
		neighbourPatch	top;
		faces		((1 0 4 5));
	}
);












