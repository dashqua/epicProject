FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273.15;
//#include "init/internalT"


boundaryField
{
  left
    {
      type cyclic;//symmetryPlane;
    }
  right
    {
      type cyclic;//Slip;//symmetryPlane;
    }
  top
    {
      type cyclic;//symmetryPlane;
    }
  bottom
    {
      type cyclic;//Slip;//symmetryPlane;
    }
  inbound
    {
      type cyclic;//slip;
    }
  outbound
    {
      type cyclic;//slip;
    }
    defaultFaces
    {
      type            empty;//slip;//wall;
    }
}


