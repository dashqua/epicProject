FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1. 0. 0.);//(.8944 .4472 0);
//#include "init/internalU"

boundaryField
{
    side
    {
      type symmetryPlane;
    }
    inbound
    {
      type slip;
    }
    outbound
    {
      type slip;
    }
    defaultFaces
    {
      type            empty;//slip;//wall;
    }
}


