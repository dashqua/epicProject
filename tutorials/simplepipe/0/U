FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (1 0 0);

boundaryField
{
    inbound
    {
      type cyclicAMI;
      value uniform (1 0 0);
    }
    outbound
    {
      type cyclicAMI;
      value uniform (1 0 0);
    }
    left
    {
      type empty;//noSlip;
    }
    right
    {
      type empty;//noSlip;
    }
    top
    {
      type noSlip;
    }
    bottom
    {
      type noSlip;
    }
}


