FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    location    "0";
    object      MDN;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inbound
    {
        type            cyclic;
    }
    outbound
    {
        type            cyclic;
    }
    left
    {
        type            calculated;
    }
    right
    {
        type            calculated;
    }
    top
    {
        type            calculated;
    }
    bottom
    {
        type            calculated;
    }
    global
    {
        type            global;
    }
}


