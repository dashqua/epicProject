FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0;
// #include "init/internalp"

boundaryField
{
  left
    {
      type zeroGradient;
    }
  right
    {
      type zeroGradient;
    }
  top
    {
      type zeroGradient;
    }
  bottom
    {
      type zeroGradient;
    }
  inbound
    {
      type cyclicAMI;
      value uniform 0;
    }
  outbound
    {
      type cyclicAMI;
      value uniform 0;
    }
}


