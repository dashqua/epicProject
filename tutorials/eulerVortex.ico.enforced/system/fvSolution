FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    p
    {
      solver PBiCGStab;
      preconditioner DILU;//DIC;//DILU;
      tolerance 1e-06;
      relTol 0.01;
    }

    "(rho|rhoU|rhoE)"
    {
        solver          diagonal;
	tolerance 1e-05;
	relTol 0;
    }

    "(U|h|e|R|k|epsilon|omega)"
    {
      solver smoothSolver;
      smoother symGaussSeidel;
      tolerance 1e-05;
      relTol 0.1;
    }
    pFinal
    {
        $p;
        relTol          0;
    }

}


PIMPLE
{
    momentumPredictor   yes;
    correctPhi          yes;
    nOuterCorrectors    2;
    nCorrectors         2;
    transonic           yes;
    nNonOrthogonalCorrectors 0;

    rhoMin              0.1;
    rhoMax              100.0;
}

relaxationFactors
{
    equations
    {
        ".*"        1;
    }
}

SIMPLE
{
    nNonOrthogonalCorrectors 0;
    consistent yes;
}
PISO
{
    nCorrectors     2;
    nNonOrthogonalCorrectors 2;
}





  
