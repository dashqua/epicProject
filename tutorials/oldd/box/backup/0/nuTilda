FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nuTilda;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    "(bottom_cyc|top_cyc|inlet_cyc|outlet_cyc)"
    {
        type            cyclicAMI;
        value           $internalField;
    }
 
    emptyPatches_empt
    {
        type           zeroGradient;
    }
        

}

