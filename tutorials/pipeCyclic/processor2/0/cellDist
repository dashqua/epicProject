FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      cellDist;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    inlet
    {
        type            calculated;
        value           nonuniform 0();
    }
    outlet
    {
        type            calculated;
        value           uniform 0;
    }
    side1
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    side2
    {
        type            cyclicAMI;
        value           uniform 0;
    }
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 3;
    }
}


