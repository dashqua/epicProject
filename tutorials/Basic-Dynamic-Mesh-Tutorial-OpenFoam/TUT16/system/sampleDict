FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

setFormat gnuplot;
formatOptions{ ensight{ format ascii; } }
interpolationScheme cellPatchConstrained;

fields ( p mag(U));

sets
(
